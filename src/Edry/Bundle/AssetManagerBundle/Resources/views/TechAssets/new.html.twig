{% extends 'EdryAssetManagerBundle::layout.html.twig' %}

{% block title %}New Asset{% endblock %}

{% block content %}
<h1>New Asset</h1>

<form action="{{ path('techassets_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('techassets') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}