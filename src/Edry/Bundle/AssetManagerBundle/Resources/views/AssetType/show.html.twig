<h1>AssetType</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Category</th>
            <td>{{ entity.category }}</td>
        </tr>
        <tr>
            <th>Model</th>
            <td>{{ entity.model }}</td>
        </tr>
        <tr>
            <th>Manufacturer</th>
            <td>{{ entity.manufacturer }}</td>
        </tr>
        <tr>
            <th>Cost</th>
            <td>{{ entity.cost }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('assettype') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('assettype_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('assettype_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
