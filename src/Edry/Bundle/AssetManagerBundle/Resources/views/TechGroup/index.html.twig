<h1>TechGroup list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Van_rego</th>
            <th>Location</th>
            <th>Driver</th>
            <th>Timestamp</th>
            <th>Make</th>
            <th>Car_year</th>
            <th>Odo</th>
            <th>Vin</th>
            <th>Condition_description</th>
            <th>Fuel_card</th>
            <th>Tag_time</th>
            <th>Radio_code</th>
            <th>Imarda_tag</th>
            <th>Van_tracker</th>
            <th>Tow_bar</th>
            <th>Spare_key</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('techgroup_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.vanrego }}</td>
            <td>{{ entity.location }}</td>
            <td>{{ entity.driver }}</td>
            <td>{% if entity.timestamp %}{{ entity.timestamp|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.make }}</td>
            <td>{{ entity.caryear }}</td>
            <td>{{ entity.odo }}</td>
            <td>{{ entity.vin }}</td>
            <td>{{ entity.conditiondescription }}</td>
            <td>{{ entity.fuelcard }}</td>
            <td>{{ entity.tagtime }}</td>
            <td>{{ entity.radiocode }}</td>
            <td>{{ entity.imardatag }}</td>
            <td>{{ entity.vantracker }}</td>
            <td>{{ entity.towbar }}</td>
            <td>{{ entity.sparekey }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('techgroup_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('techgroup_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('techgroup_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
